CFLAGS  := -O2 -Wall -c
CC := gcc
RM := rm -f

NAME1 := wykluczanie

OBJ1 := $(NAME1).o

.PHONY: all clean

#-----------------------------------------------------------------------------------------

all: wykluczanie.x 

#-----------------------------------------------------------------------------------------

%.x: %.o
	$(CC) -o $@ $^ -lpthread
%.o: %.c
	$(CC) $(CFLAGS) -o $@ $<

#-----------------------------------------------------------------------------------------

clean:
	$(RM) *.o *.x 
